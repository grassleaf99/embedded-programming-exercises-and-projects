#include <main.h>

void main()
{
   set_tris_d(0x00); // cài dat port D làm output
   while(TRUE)
   {
      output_d(0xFE); // den led 1 (chi so 0) sang
      delay(1000);    // tam dung chuong trinh 1000 ms = 1s (tham so truyen vao là ms)
      output_d(0x00); // tat ca den deu sang
      delay(1000);
      output_d(0xFF); // tat ca den deu tat
      delay(1000);
   }

}
