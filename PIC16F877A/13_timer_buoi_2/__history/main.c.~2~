#include <main.h>
long count = 0;
void main()
{
   set_tris_d(0x00);
   setup_timer_1(T1_INTERNAL | T1_DIV_BY_8);
   setup_timer1(64286);
   enable_interrupts(INT_TIMER1);
   enable_interrupts(GLOBAL)
   while(TRUE)
   {
      //TODO: User Code
   }

}

#INT_TIMER1
void chuyenled()
{
     setup_timer1(64286);
     count++;
     if(count == 1000)
     {
         count = 0;
         output_toggle(PIN_D0);
     }
}
